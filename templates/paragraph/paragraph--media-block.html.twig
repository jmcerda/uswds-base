{% block paragraph %}
  {%
    set classes = [
    'usa-media-block',
    'tablet:grid-col',
  ]
  %}
  <div{{ attributes.addClass(classes) }}>
    {% block content %}
{#      <img class="usa-media-block__img"  src="{{ file_url(paragraph.field_image.entity.field_media_image.entity.fileuri | image_style('media_block')) }}" alt="{{ paragraph.field_image.entity.field_media_image.alt }}">#}
      <img class="usa-media-block__img"  src="{{ file_url(paragraph.field_image.entity.field_media_image.entity.fileuri) }}" alt="{{ paragraph.field_image.entity.field_media_image.alt }}">
{#      <div class="usa-media-block__img">{{ content.field_image }}</div>#}
      <div class="usa-media-block__body">
        <h2 class="usa-graphic-list__heading">{{ paragraph.field_title.value }}</h2>
        {{ content|without('field_image', 'field_title') }}
      </div>
    {% endblock %}
  </div>
{% endblock paragraph %}
